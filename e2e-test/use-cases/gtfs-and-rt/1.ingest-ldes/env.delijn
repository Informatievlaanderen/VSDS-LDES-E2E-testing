GTFS_SOURCE=/data/delijn.latest.zip # download from https://vsdscore.slack.com/files/U03LA44NALW/F03P0EB4T2A/delijn.latest.zip into GTFS2LDES_DATA_FOLDER
GTFSRT_SOURCE=https://api.delijn.be/gtfs/v1/realtime
SPRING_DATA_MONGODB_DATABASE=delijn
MONGODB_DATA_FOLDER=~/data/gtfs/delijn/mongo.db/ # on windows use: "%USERPROFILE%/data/gtfs/delijn/mongo.db/"
AUTH_HEADER=Ocp-Apim-Subscription-Key
AUTH_HEADER_VALUE=REPLACE-ME # I.e. get your own subscription @ https://data.delijn.be/docs/services/
