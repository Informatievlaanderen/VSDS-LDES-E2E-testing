# Use case
# USECASE_NAME=gtfs-geo-fragment

# GTFS2LDES compose env vars

# Mandatory
# GTFS_SOURCE=
# GTFSRT_SOURCE=
# GTFS2LDES_DATA_FOLDER=
# GTFS_BASE_IRI=

# Optional
# GTFS2LDES_TAG=20220928t1555
# RUN_ON_LAUNCH=true
# THROTTLE_RATE=100
# GTFS_CRON="0 0 3 15 * *"
# GTFSRT_CRON="*/30 * * * * *"
# AUTH_HEADER=
# AUTH_HEADER_VALUE=


# Workflow compose env vars

# Mandatory
# SINGLE_USER_CREDENTIALS_USERNAME=
# SINGLE_USER_CREDENTIALS_PASSWORD=

# Optional
# NIFI_TAG=1.17.0-jdk17
# NIFI_JVM_HEAP_INIT=2g
# NIFI_JVM_HEAP_MAX=4g
# NIFI_UI_PORT=8443
# NIFI_WORKFLOW_LISTEN_PORT=9005


# Mongo compose env vars

# Mandatory
# MONGODB_DATA_FOLDER=

# Optional
# MONGODB_TAG=5.0.12
# MONGODB_PORT=27017


# Server compose env vars

# Mandatory
# LDES_SERVER_TAG=
# SPRING_DATA_MONGODB_DATABASE=

# Optional
# LDES_SERVER_PORT=8080
# LDES_COLLECTIONNAME="connections"
# LDES_MEMBERTYPE="http://semweb.mmlab.be/ns/linkedconnections#Connection"
# LDES_SHAPE=
# VIEW_TIMESTAMPPATH="http://www.w3.org/ns/prov#generatedAtTime"
# VIEW_VERSIONOFPATH="http://purl.org/dc/terms/isVersionOf"

# Views
# Geospatial View
