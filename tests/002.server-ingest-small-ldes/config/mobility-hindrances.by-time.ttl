@prefix tree:   <https://w3id.org/tree#>.
@prefix server: <http://example.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

@base <http://localhost:8080> .

</mobility-hindrances/by-time> a tree:Node ; 
  tree:viewDescription [ a tree:ViewDescription ;
    tree:fragmentationStrategy ([
		a server:TimebasedFragmentation ;
		tree:memberLimit "1500"^^xsd:integer ;
		tree:fragmentationPath <http://www.w3.org/ns/prov#generatedAtTime> ;
	]) ;
] .
