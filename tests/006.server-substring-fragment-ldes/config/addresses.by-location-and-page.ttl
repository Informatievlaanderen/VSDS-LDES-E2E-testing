@prefix tree: <https://w3id.org/tree#>.
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

@base <http://localhost:8080> .

</addresses/by-location-and-page> a tree:Node ; 
	tree:viewDescription [ a tree:ViewDescription ;
		tree:fragmentationStrategy (
		[
			a tree:GeospatialFragmentation ;
			tree:fragmentationPath <http://www.opengis.net/ont/geosparql#asGML> ;
			tree:maxZoom "13"^^xsd:integer ;
		]
		[
            a tree:PaginationFragmentation ;
		    tree:memberLimit "10"^^xsd:integer ;
		]) ;
	] .
