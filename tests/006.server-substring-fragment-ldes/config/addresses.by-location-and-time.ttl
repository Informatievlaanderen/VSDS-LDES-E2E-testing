@prefix tree: <https://w3id.org/tree#>.
@prefix server: <http://example.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

@base <http://localhost:8080> .

</addresses/by-location-and-time> a tree:Node ; 
	tree:viewDescription [ a tree:ViewDescription ;
		server:fragmentationStrategy ([
			a server:Fragmentation ;
			server:name "geospatial" ;
			server:projection "noop" ;
			server:fragmenterProperty "http://www.opengis.net/ont/geosparql#asGML" ;
			server:maxZoomLevel "13"^^xsd:integer ;
		]) ;
	] .
	
