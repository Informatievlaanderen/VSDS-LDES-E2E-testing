@prefix tree: <https://w3id.org/tree#>.
@prefix server: <http://example.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

@base <http://localhost:8080> .

</mobility-hindrances/by-location-and-time> a tree:Node ; 
  tree:viewDescription [ a tree:ViewDescription ;
    server:fragmentationStrategy ([
		a server:Fragmentation ;
		server:name "geospatial" ;
		server:projection "lambert72" ;
		server:maxZoomLevel "15"^^xsd:integer ;
		server:fragmenterProperty "http://www.opengis.net/ont/geosparql#asWKT" ;
	]) ;
] .
