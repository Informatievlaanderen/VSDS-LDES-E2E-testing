@prefix tree:   <https://w3id.org/tree#>.
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

@base <http://localhost:8080> .

</connections/by-location-and-time> a tree:Node ; 
  tree:viewDescription [ a tree:ViewDescription ;
    tree:fragmentationStrategy (
    [
		a tree:GeospatialFragmentation; ;
		tree:maxZoom "15"^^xsd:integer ;
		tree:fragmentationPath "http://www.opengis.net/ont/geosparql#asWKT" ;
	]
	[
        a tree:TimebasedFragmentation ;
        tree:memberLimit "100"^^xsd:integer ;
		tree:fragmentationPath <http://www.w3.org/ns/prov#generatedAtTime> ;
	]
	) ;
] .
